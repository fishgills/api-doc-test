                "CurrGateway": "192.168.1.1",
                "CurrDNS1": "8.8.8.8",
                "CurrDNS2": "8.8.4.4",
                "StaticIP": "192.168.1.20",
                "StaticNetmask": "255.255.255.0",
                "StaticGateway": "192.168.1.1",
                "StaticDNS1": "8.8.8.8",
                "StaticDNS2": "8.8.4.4",
                "Curr": {
                  "IP": "192.168.1.20",
                  "Netmask": "255.255.255.0",
                  "Gateway": "192.168.1.1",
                  "PrimaryDNS": "8.8.8.8",
                  "SecondaryDNS": "8.8.4.4"
                },
                "Static": {
                  "IP": "192.168.1.20",
                  "Netmask": "255.255.255.0",
                  "Gateway": "192.168.1.1",
